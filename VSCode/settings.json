{
    "files.trimTrailingWhitespace": true,
    "vim.foldfix": true,
    "window.zoomLevel": 1,
    "editor.fontSize": 16,
    "editor.fontFamily": "Ubuntu Mono,Fira Mono, Menlo, Monaco, 'Courier New', monospace",
    "terminal.integrated.fontSize": 16,
    "vim.normalModeKeyBindings": [
        {
            "before": ["leader", "k"],
            "after": ["i", "<Enter>", "<Esc>"],
        },
        {
            "before": [";", "w"],
            "after": [":", "w", "<Enter>"],
        },
    ],
    "vim.insertModeKeyBindings": [
        {
            "before": ["k", "k"],
            "after": ["<Esc>"]
        },
        {
            "before": ["j", "j"],
            "after": ["<Esc>"]
        },
        {
            "before": ["j", "v"],
            "after": ["<Esc>", "v"]
        }
    ],
    "vim.leader": ";",
    "breadcrumbs.enabled": true,
    "workbench.sideBar.location": "left",
    "editor.minimap.renderCharacters": false,
    "explorer.openEditors.visible": 0,
    "diffEditor.ignoreTrimWhitespace": true,
    "diffEditor.renderSideBySide": true,
    "editor.acceptSuggestionOnEnter": "smart",
    "vim.easymotion": true,
    "vim.easymotionMarkerFontFamily": "Fira Mono, Ubuntu Mono, Menlo, Monaco, 'Courier New', monospace",
    "vim.easymotionMarkerFontSize": "16",
    "[html]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "editor.tabSize": 2,
    "workbench.panel.defaultLocation": "right",
    "sshfs.configs": [
        {
            "host": "192.168.20.34",
            "label": "asu",
            "name": "asus",
            "password": true,
            "root": "/home/keang/work",
            "username": "keang"
        },
        {
            "host": "192.168.20.34",
            "name": "acer",
            "password": true,
            "root": "/home/keang/src",
            "username": "keang"
        }
    ],
    "vim.easymotionMarkerYOffset": null,
    "vim.easymotionMarkerHeight": 20,
    "gitlens.views.repositories.location": "scm",
    "gitlens.views.fileHistory.location": "explorer",
    "gitlens.views.lineHistory.location": "explorer",
    "gitlens.views.compare.location": "gitlens",
    "gitlens.views.search.location": "gitlens",
    "gitlens.hovers.currentLine.over": "line",
    "gitlens.codeLens.enabled": false,
    "zenMode.fullScreen": false,
    "zenMode.restore": true,
    "workbench.startupEditor": "newUntitledFile",
    "extensions.ignoreRecommendations": false,
    "editor.find.autoFindInSelection": true,
    "zenMode.hideLineNumbers": false,
    "workbench.colorTheme": "Tomorrow Night Blue",
    "editor.minimap.enabled": false,
}